            <ul id="navigation" class="naturescot-header__navigation {{ params.navigationClasses if params.navigationClasses }}" aria-label="Top Level Navigation">
              {% for item in params.navigation %}
                {% if item.href and item.text %}
                  <li class="naturescot-header__navigation-item{{ ' naturescot-header__navigation-item--active' if item.active }}">
                    <a class="naturescot-header__link" href="{{ item.href }}"{% for attribute, value in item.attributes %} {{attribute}}="{{value}}"{% endfor %}>
                      {{ item.text }}
                    </a>
                  </li>
                {% endif %}
              {% endfor %}
            </ul>
          </nav>
        {% endif %}
      </div>
    {% endif %}
